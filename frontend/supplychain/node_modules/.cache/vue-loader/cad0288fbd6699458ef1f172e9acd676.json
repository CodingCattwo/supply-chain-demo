/el-collapse-item>\n        <el-collapse-item title=\"描述\" name=\"3\">\n          <div>{{ userData.orgDescription }}</div>\n        </el-collapse-item>\n        <el-collapse-item title=\"余额\" name=\"4\">\n          <div style=\"color:red;font-weight:blod;font-size:16px\">\n            {{ userData.balance / 100 }}\n          </div>\n        </el-collapse-item>\n      </el-collapse>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { getUser } from \"@/api/table\";\nimport { getUserId } from \"@/utils/auth\";\n\nexport default {\n  data() {\n    return {\n      userData: {},\n      activeNames: [\"1\", \"2\", \"3\", \"4\"]\n    };\n  },\n  created() {\n    this.getUserInfo();\n  },\n  methods: {\n    getUserInfo() {\n      const id = getUserId();\n      getUser(id).then(res => {\n        this.userData = res.data;\n      });\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\">\n.el-collapse {\n  font-size: 16px;\n}\n</style>\n"]}]}