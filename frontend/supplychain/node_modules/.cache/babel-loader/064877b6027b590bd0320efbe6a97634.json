AA,CAAA,QAAA,CAAA,CAAA,EAAA,KAAA,CAAA,CAAA,EAAA,cAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,KAAA,CAAA,QAAA,CAAA,CAAA,EAAA,KAAA;AACA,OALA;AAMA,KAVA;AAWA,IAAA,OAXA,mBAWA,OAXA,EAWA;AAAA;;AACA,UAAA,EAAA,GAAA,OAAA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,OAAA,KAAA,EAAA,EAAA;AACA,UAAA,MAAA,CAAA,YAAA,GAAA,IAAA,CAAA,KAAA;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,cAAA,GAAA,IAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA;AACA;AACA,OANA;AAOA,KApBA;AAqBA,IAAA,YArBA,0BAqBA;AAAA;;AACA,UAAA,MAAA,GAAA,KAAA,IAAA;AACA,MAAA,OAAA,CAAA,MAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,cAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,UAAA,MAAA,CAAA,MAAA,GAAA,OAAA;AACA,UAAA,MAAA,CAAA,mBAAA,GAAA,IAAA;AACA;AACA,OANA;AAOA;AA9BA;AAhBA,CAAA","sourcesContent":["<template>\n  <div class=\"app-container\">\n    <el-form ref=\"form\" :model=\"form\" label-width=\"120px\" class=\"form-box\">\n      <el-form-item label=\"链路选择\">\n        <el-select\n          v-model=\"form.chainId\"\n          placeholder=\"链路选择\"\n          @change=\"getRole\"\n        >\n          <el-option\n            v-for=\"item in signData\"\n            :label=\"item.title\"\n            :value=\"item.chainId\"\n            :key=\"item.chainId\"\n          />\n        </el-select>\n      </el-form-item>\n      <el-form-item label=\"角色选择\">\n        <el-select v-model=\"form.participaterId\" placeholder=\"角色选择\">\n          <el-option\n            v-for=\"item in relationRole\"\n            :label=\"item.role\"\n            :value=\"item.participaterId\"\n            :key=\"item.participaterId\"\n          />\n        </el-select>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"handleSigned\">提交</el-button>\n        <el-button>取消</el-button>\n      </el-form-item>\n    </el-form>\n    <el-dialog title=\"提示\" :visible.sync=\"txHashdialogVisible\" width=\"30%\">\n      <div class=\"trade-tips\">\n        <i class=\"el-icon-circle-check\" style=\"clolor:green\"></i>签名成功\n      </div>\n      <div class=\"title\">交易hash:</div>\n      <span>{{ txHash }}</span>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button type=\"primary\" @click=\"txHashdialogVisible = false\"\n          >确 定</el-button\n        >\n      </span>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { getSignInfo, addSign } from \"@/api/table\";\nimport { getUserId } from \"@/utils/auth\";\n\nexport default {\n  data() {\n    return {\n      txHashdialogVisible: false,\n      signData: [],\n      relationRole: [],\n      txHash: \"\",\n      form: {\n        chainId: \"\",\n        participaterId: \"\"\n      }\n    };\n  },\n  created() {\n    this.getSignData();\n  },\n  methods: {\n    getSignData() {\n      const id = getUserId();\n\n      getSignInfo(id).then(res => {\n        this.signData = res.data;\n        this.form.chainId = this.signData[0].chainId;\n        this.form.participaterId = this.signData[0].roles[0].participaterId;\n        this.relationRole = this.signData[0].roles;\n      });\n    },\n    getRole(chainId) {\n      const id = chainId;\n      this.signData.forEach(item => {\n        if (item.chainId === id) {\n          this.relationRole = item.roles;\n          this.form.participaterId = item.roles[0];\n          return;\n        }\n      });\n    },\n    handleSigned() {\n      const params = this.form;\n      addSign(params).then(res => {\n        if (res.code === 0) {\n          const Tx_hash = res.data.txHash;\n          this.txHash = Tx_hash;\n          this.txHashdialogVisible = true;\n        }\n      });\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.trade-tips {\n  font-size: 16px;\n  padding-bottom: 30px;\n  text-align: center;\n  color: #000;\n  i {\n    color: #67c23a;\n    padding-right: 10px;\n    font-size: 18px;\n  }\n}\n.el-select {\n  width: 100%;\n}\n.form-box {\n  width: 40%;\n}\n</style>\n"],"sourceRoot":"src/views/sign"}]}